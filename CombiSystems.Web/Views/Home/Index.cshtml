@{
    Layout = null;
}
<head>
    <!-- basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>Fornax</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- site icon -->
    <link rel="icon" href="../../root/images/fevicon.png" type="image/png">
    <!-- bootstrap css -->
    <link rel="stylesheet" href="../../root/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
    <!-- site css -->
    <link rel="stylesheet" href="../../root/style.css">
    <!-- responsive css -->
    <link rel="stylesheet" href="../../root/css/responsive.css">
    <!-- color css -->
    <link rel="stylesheet" href="../../root/css/colors.css">
    <!-- select bootstrap -->
    <link rel="stylesheet" href="../../root/css/bootstrap-select.css">
    <!-- scrollbar css -->
    <link rel="stylesheet" href="../../root/css/perfect-scrollbar.css">
    <!-- custom css -->
    <link rel="stylesheet" href="../../root/css/custom.css">
    <!-- calendar file css -->
    <link rel="stylesheet" href="../../root/js/semantic.min.css">
    <!-- fancy box js -->
    <link rel="stylesheet" href="../../root/css/jquery.fancybox.css">
    <!--[if lt IE 9]>

    <![endif]-->
</head>
<body class="inner_page tables_page">
    <div class="full_container">
        <div class="inner_container">

            <div class="topbar">
                <nav class="navbar navbar-expand-lg navbar-light">
                    <div class="full">

                        <div class="logo_section">
                            <a href="index.html"> <img class="img-responsive" src="../../root/images/logo/fornax.svg" alt="#"> </a>
                        </div>
                        <div class="right_topbar">
                            @if (!@Context.User.Identity!.IsAuthenticated)
                            {
                                <div class="icon_info">

                                    <ul class="user_profile_aa mx-3">
                                        <li>

                                            <a class="btn btn-primary logreg" style="width:90px" href="/Login">Login</a>
                                            <a class="btn btn-success logreg" style="width:90px" href="/Register">Register</a>

                                             
                                        </li>
                                    </ul>

                                </div>
                            }
                            else
                            {

                                <div class="icon_info ">
                                    <ul class="user_profile_dd">
                                        <li>
                                            <a class="dropdown-toggle" data-toggle="dropdown">
                                                <img class="img-responsive rounded-circle" src="https://www.bootdey.com/app/webroot/img/Content/icons/64/PNG/64/packman.png" alt="#" />
                                                <span class="name_user">Erhan</span>
                                            </a>
                                            <div class="dropdown-menu">
                                                <a class="dropdown-item" asp-controller="User" asp-action="Profile">My Profile</a>
                                                <a class="dropdown-item" asp-controller="User" asp-action="Appointment">Appointment</a>
                                                <a class="dropdown-item" href="help.html">Help</a>
                                                <a class="dropdown-item" asp-controller="Home" asp-action="Logout"><span>Log Out</span> <i class="fa fa-sign-out"></i></a>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            }
                        </div>
                    </div>
                </nav>
            </div>
            <div id="content">

                <div class="midde_cont">

                    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                        <ol class="carousel-indicators">
                            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                            <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
                            <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
                        </ol>
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img class="d-block w-100" src="../../root/images/bg2.jpeg" alt="First slide">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="../../root/images/bg2.jpeg" alt="Second slide">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="../../root/images/bg2.jpeg" alt="Third slide">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="../../root/images/bg2.jpeg" alt="Fourth slide">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block w-100" src="../../root/images/bg2.jpeg" alt="Fifth slide">
                            </div>

                        </div>
                        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>

                </div>


                <div class="footer">
                    <p> Copyright © 2018 Designed by html.design. All rights reserved.</p>
                </div>
            </div>
            <!-- footer -->
            <!-- Endfooter -->
        </div>
        <!-- end dashboard inner -->
    </div>

    <!-- model popup -->




    <style>
        .topbar {
            padding-left: 0 !important;
        }

        #content {
            padding: 60px 25px 25px 30px !important;
        }

    </style>
    <style>


    </style>

    <script type="text/javascript">
        $(document).ready(function() {
          $('.carousel').carousel({
            interval: 3500
          })
        });
    </script>

</body>